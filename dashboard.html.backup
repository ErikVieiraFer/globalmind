    <script>
        // Chart.js Configuration
        Chart.defaults.font.family = "'Source Sans 3', sans-serif";
        Chart.defaults.color = '#6c757d';

        // Radar Chart - Distribuição por Dimensão
        const radarCtx = document.getElementById('radarChart').getContext('2d');
        new Chart(radarCtx, {
            type: 'radar',
            data: {
                labels: ['Neuroemocional', 'Organizacional', 'Liderança', 'Cultura', 'Riscos PSS'],
                datasets: [{
                    label: 'Pontuação',
                    data: [7.2, 6.8, 5.5, 7.8, 5.8],
                    backgroundColor: 'rgba(109, 113, 65, 0.2)',
                    borderColor: '#6D7141',
                    borderWidth: 2,
                    pointBackgroundColor: '#6D7141',
                    pointBorderColor: '#fff',
                    pointHoverBackgroundColor: '#fff',
                    pointHoverBorderColor: '#6D7141'
                }]
            },
            options: {
                responsive: true,
                maintainAspectRatio: false,
                scales: {
                    r: {
                        beginAtZero: true,
                        max: 10,
                        ticks: {
                            stepSize: 2,
                            font: { size: 11 }
                        },
                        pointLabels: {
                            font: { size: 12, weight: '500' },
                            color: '#1E2C36'
                        },
                        grid: {
                            color: 'rgba(30, 44, 54, 0.1)'
                        },
                        angleLines: {
                            color: 'rgba(30, 44, 54, 0.1)'
                        }
                    }
                },
                plugins: {
                    legend: {
                        display: false
                    }
                }
            }
        });

        // Bar Chart - Evolução dos Índices
        const barCtx = document.getElementById('barChart').getContext('2d');
        new Chart(barCtx, {
            type: 'bar',
            data: {
                labels: ['Sobrecarga', 'Dependência', 'Maturidade', 'Risco PSS', 'Conflito', 'Escalabil.'],
                datasets: [
                    {
                        label: 'Atual',
                        data: [8.2, 6.5, 7.1, 5.8, 3.2, 5.4],
                        backgroundColor: '#6D7141',
                        borderRadius: 6,
                        barThickness: 20
                    },
                    {
                        label: 'Anterior',
                        data: [7.5, 7.2, 6.5, 6.2, 4.1, 4.8],
                        backgroundColor: '#ced4da',
                        borderRadius: 6,
                        barThickness: 20
                    }
                ]
            },
            options: {
                responsive: true,
                maintainAspectRatio: false,
                scales: {
                    x: {
                        grid: {
                            display: false
                        },
                        ticks: {
                            font: { size: 11 }
                        }
                    },
                    y: {
                        beginAtZero: true,
                        max: 10,
                        grid: {
                            color: 'rgba(30, 44, 54, 0.08)'
                        },
                        ticks: {
                            stepSize: 2,
                            font: { size: 11 }
                        }
                    }
                },
                plugins: {
                    legend: {
                        display: false
                    }
                }
            }
        });

        // Add animation on scroll
        const observerOptions = {
            threshold: 0.1,
            rootMargin: '0px 0px -50px 0px'
        };

        const observer = new IntersectionObserver((entries) => {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    entry.target.style.opacity = '1';
                    entry.target.style.transform = 'translateY(0)';
                }
            });
        }, observerOptions);

        document.querySelectorAll('.animate-in').forEach(el => {
            el.style.opacity = '0';
            el.style.transform = 'translateY(20px)';
            el.style.transition = 'all 0.6s ease';
            observer.observe(el);
        });
    </script>
